{
  "name": "inline-css-loader",
  "version": "2.1.0",
  "author": {
    "name": "marudor",
    "email": "marudor@marudor.de"
  },
  "license": "MIT",
  "repository": "marudor/inline-css-loader",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "test": "npm run lint && npm run jest",
    "lint": "eslint *.js",
    "jest": "jest"
  },
  "jest": {
    "verbose": true,
    "noStackTrace": true,
    "automock": false,
    "testPathDirs": [
      "src"
    ]
